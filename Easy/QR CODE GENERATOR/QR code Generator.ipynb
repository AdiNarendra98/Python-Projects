    "import qrcode\n",
    "import image\n",
    "qr=qrcode.QRCode(\n",
    "version=15,\n",
    "box_size=10,\n",
    "border=5)\n",
    "data=str(input(\"Enter a Link:\"))\n",
    "qr.add_data(data)\n",
    "qr.make(fit=True)\n",
    "img=qr.make_image(fill=\"black\",back_color=\"white\")\n",
    "img.save(\"QR CODE\")\n",
    "print(\"QR CODE SAVED\")\n",
    "\n",
    "\n"
   ]
